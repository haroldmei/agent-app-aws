# Environment configuration for agent tests
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=ai
DB_PASS=ai
DB_DATABASE=ai

# Test Configuration
TEST_USER_ID=test_user
TEST_MODEL_ID=gpt-4o-mini

# Optional: Enable debug mode for agents during testing
AGENT_DEBUG_MODE=false

# Optional: Test timeout settings (in seconds)
TEST_TIMEOUT=60
RESPONSE_TIMEOUT=30
